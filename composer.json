{
    "name": "zendframework/zenddiagnostics",
    "description": "A set of interfaces for performing diagnostic tests for PHP applications",
    "keywords": [
        "php",
        "diagnostics",
        "test"
    ],
    "homepage": "https://github.com/zendframework/ZendDiagnostics",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-0": {
            "ZendDiagnostics\\": "src/",
            "ZendDiagnosticsTest\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "zendframework/zend-loader": "2.*"
    },
    "suggest" : {
        "ext-bcmath" : "Required by Check\\CpuPerformance"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
